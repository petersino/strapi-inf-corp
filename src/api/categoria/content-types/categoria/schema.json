{
  "kind": "collectionType",
  "collectionName": "categorias",
  "info": {
    "singularName": "categoria",
    "pluralName": "categorias",
    "displayName": "Categoria"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Nombre": {
      "type": "string",
      "default": "nombre de la categoria",
      "required": true,
      "unique": true
    },
    "URL": {
      "type": "uid",
      "targetField": "Nombre",
      "required": true
    },
    "Imagen": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "subcategorias": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subcategoria.subcategoria",
      "mappedBy": "categoria"
    }
  }
}
